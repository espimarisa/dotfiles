{
	"[caddyfile]": {
		"editor.defaultFormatter": "matthewpi.caddyfile-support"
	},
	"[dockercompose][yaml]": {
		"editor.autoIndent": "advanced",
		"editor.codeLens": false,
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.insertSpaces": true,
		"editor.quickSuggestions": {
			"comments": false,
			"other": true,
			"strings": true
		},
		"editor.tabSize": 2,
		"prettier.tabWidth": 2,
		"prettier.useTabs": false
	},
	"[dotenv]": {
		"editor.defaultFormatter": "lkrms.inifmt"
	},
	"[github-actions-workflow]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[shellscript]": {
		"editor.defaultFormatter": "mkhl.shfmt"
	},
	"[toml]": {
		"editor.defaultFormatter": "tamasfe.even-better-toml"
	},
	"astro.updateImportsOnFileMove.enabled": true,
	"basedpyright.importStrategy": "useBundled",
	"biome.requireConfiguration": true,
	"biome.suggestInstallingGlobally": false,
	"breadcrumbs.enabled": false,
	"chat.agent.enabled": false,
	"chat.commandCenter.enabled": false,
	"chat.disableAIFeatures": true,
	"chat.emptyChatState.enabled": false,
	"chat.emptyState.history.enabled": false,
	"colorize.colorized_variables": ["CSS", "LESS", "SASS", "STYLUS"],
	"colorize.enable_search_variables": true,
	"colorize.hide_current_line_decorations": false,
	"colorize.languages": [
		"css",
		"csv",
		"handlebars",
		"html",
		"javascript",
		"javascriptreact",
		"json",
		"json5",
		"jsonc",
		"jsonl",
		"less",
		"markdown",
		"nunjucks",
		"njk",
		"plaintext",
		"pug",
		"sass",
		"scss",
		"stylus",
		"svelte",
		"svg",
		"vue",
		"vue-html",
		"xml",
		"yaml"
	],
	"docker.extension.enableComposeLanguageServer": false,
	"editor.autoIndent": "advanced",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.fontFamily": "Noto Sans Mono Medium",
	"editor.fontLigatures": true,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": true,
	"editor.insertSpaces": false,
	"editor.lineHeight": 1.5,
	"editor.linkedEditing": true,
	"editor.matchBrackets": "always",
	"editor.minimap.autohide": "mouseover",
	"editor.semanticHighlighting.enabled": true,
	"editor.stickyScroll.enabled": false,
	"editor.tabSize": 4,
	"editor.wordWrap": "on",
	"emmet.includeLanguages": {
		"njk": "html",
		"nunjucks": "html"
	},
	"explorer.compactFolders": false,
	"files.associations": {
		"*.env*": "dotenv"
	},
	"files.autoSave": "afterDelay",
	"files.autoSaveDelay": 5000,
	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
	"git.openRepositoryInParentFolders": "always",
	"javascript.preferences.importModuleSpecifier": "relative",
	"javascript.suggest.paths": false,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"latex-utilities.message.update.show": false,
	"latex-workshop.formatting.latex": "tex-fmt",
	"latex-workshop.formatting.tex-fmt.args": [
		"--indent",
		"--indent-size",
		"--nowrap",
		"4"
	],
	"prettier.arrowParens": "always",
	"prettier.bracketSameLine": true,
	"prettier.bracketSpacing": true,
	"prettier.embeddedLanguageFormatting": "auto",
	"prettier.endOfLine": "lf",
	"prettier.experimentalTernaries": true,
	"prettier.htmlWhitespaceSensitivity": "css",
	"prettier.jsxSingleQuote": false,
	"prettier.printWidth": 80,
	"prettier.proseWrap": "preserve",
	"prettier.quoteProps": "consistent",
	"prettier.semi": true,
	"prettier.singleAttributePerLine": false,
	"prettier.singleQuote": false,
	"prettier.tabWidth": 4,
	"prettier.trailingComma": "all",
	"prettier.useTabs": true,
	"prettier.vueIndentScriptAndStyle": true,
	"redhat.telemetry.enabled": false,
	"security.workspace.trust.enabled": false,
	"security.workspace.trust.startupPrompt": "never",
	"shellcheck.customArgs": [],
	"shellcheck.enable": true,
	"shellcheck.enableQuickFix": true,
	"shellcheck.exclude": [],
	"shellcheck.executablePath": "",
	"shellcheck.ignoreFileSchemes": ["git", "gitfs", "output"],
	"shellcheck.ignorePatterns": {
		"**/*.csh": true,
		"**/*.cshrc": true,
		"**/*.fish": true,
		"**/*.login": true,
		"**/*.logout": true,
		"**/*.tcsh": true,
		"**/*.tcshrc": true,
		"**/*.xonshrc": true,
		"**/*.xsh": true,
		"**/*.zlogin": true,
		"**/*.zlogout": true,
		"**/*.zprofile": true,
		"**/*.zsh": true,
		"**/*.zsh-theme": true,
		"**/*.zshenv": true,
		"**/*.zshrc": true,
		"**/zlogin": true,
		"**/zlogout": true,
		"**/zprofile": true,
		"**/zshenv": true,
		"**/zshrc": true
	},
	"shellcheck.run": "onType",
	"svelte.enable-ts-plugin": true,
	"telemetry.feedback.enabled": false,
	"telemetry.telemetryLevel": "off",
	"terminal.integrated.stickyScroll.enabled": false,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.preferences.importModuleSpecifier": "relative",
	"typescript.preferences.importModuleSpecifierEnding": "auto",
	"typescript.preferences.useAliasesForRenames": false,
	"typescript.suggest.paths": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"typescript.updateImportsOnPaste.enabled": true,
	"update.showReleaseNotes": false,
	"vscord.app.name": "VSCodium",
	"vscord.status.idle.check": false,
	"window.commandCenter": false,
	"window.customTitleBarVisibility": "never",
	"window.titleBarStyle": "native",
	"workbench.colorTheme": "GitHub Dark",
	"workbench.editor.empty.hint": "hidden",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.layoutControl.enabled": false,
	"workbench.navigationControl.enabled": false,
	"workbench.productIconTheme": "material-product-icons",
	"workbench.secondarySideBar.defaultVisibility": "hidden",
	"workbench.startupEditor": "none",
	"workbench.tips.enabled": false,
	"workbench.tree.indent": 16,
	"yaml.schemas": {
		"https://raw.githubusercontent.com/compose-spec/compose-spec/master/schema/compose-spec.json": "*compose*.yml"
	}
}
