{
	"[caddyfile]": {
		"editor.defaultFormatter": "matthewpi.caddyfile-support"
	},
	"[dockercompose][yaml]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"prettier.tabWidth": 2,
		"prettier.useTabs": false
	},
	"[java]": {
		"editor.defaultFormatter": "redhat.java"
	},
	"[latex]": {
		"editor.defaultFormatter": "James-Yu.latex-workshop"
	},
	"[liquid]": {
		"editor.defaultFormatter": "Shopify.theme-check-vscode"
	},
	"[markdown]": {
		"diffEditor.ignoreTrimWhitespace": false,
		"editor.unicodeHighlight.ambiguousCharacters": false,
		"editor.unicodeHighlight.invisibleCharacters": false,
		"files.trimTrailingWhitespace": false
	},
	"[properties][dotenv][ini]": {
		"editor.defaultFormatter": "lkrms.inifmt"
	},
	"[python]": {
		"diffEditor.ignoreTrimWhitespace": false,
		"editor.defaultFormatter": "ms-python.black-formatter",
		"editor.insertSpaces": true,
		"prettier.useTabs": false
	},
	"[sass]": {
		"editor.defaultFormatter": "syler.sass-indented"
	},
	"[shellscript]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.shellcheck": "explicit"
		},
		"editor.defaultFormatter": "mkhl.shfmt"
	},
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"[toml]": {
		"editor.defaultFormatter": "tamasfe.even-better-toml"
	},
	"[typescript][typescriptreact][javascript][javascriptreact]": {
		"editor.codeActionsOnSave": {
			"source.action.useSortedKeys.biome": "explicit",
			"source.addMissingImports.ts": "explicit",
			"source.biome": "explicit",
			"source.fixAll.ts": "explicit",
			"source.organizeImports.biome": "explicit"
		}
	},
	"[xml]": {
		"editor.defaultFormatter": "redhat.vscode-xml"
	},
	"astro.updateImportsOnFileMove.enabled": true,
	"basedpyright.importStrategy": "useBundled",
	"biome.requireConfiguration": true,
	"biome.suggestInstallingGlobally": false,
	"breadcrumbs.enabled": false,
	"chat.fontSize": 16,
	"chat.tips.enabled": false,
	"colorize.colorized_variables": ["CSS", "LESS", "SASS", "STYLUS"],
	"colorize.enable_search_variables": true,
	"colorize.hide_current_line_decorations": false,
	"colorize.languages": [
		"css",
		"csv",
		"handlebars",
		"html",
		"javascript",
		"javascriptreact",
		"json",
		"json5",
		"jsonc",
		"jsonl",
		"less",
		"markdown",
		"nunjucks",
		"njk",
		"plaintext",
		"pug",
		"sass",
		"scss",
		"stylus",
		"svelte",
		"svg",
		"vue",
		"vue-html",
		"xml",
		"yaml"
	],
	"docker.extension.enableComposeLanguageServer": false,
	"docker.lsp.telemetry": "off",
	"editor.aiStats.enabled": false,
	"editor.autoIndent": "advanced",
	"editor.bracketPairColorization.enabled": true,
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.prettier": "explicit"
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.fontFamily": "Fira Code",
	"editor.fontLigatures": true,
	"editor.fontSize": 16,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": true,
	"editor.indentSize": "tabSize",
	"editor.insertSpaces": false,
	"editor.lineHeight": 1.45,
	"editor.linkedEditing": true,
	"editor.matchBrackets": "always",
	"editor.minimap.autohide": "mouseover",
	"editor.semanticHighlighting.enabled": true,
	"editor.stickyScroll.enabled": false,
	"editor.tabSize": 4,
	"editor.wordWrap": "on",
	"emmet.includeLanguages": {
		"njk": "html",
		"nunjucks": "html"
	},
	"explorer.compactFolders": false,
	"files.associations": {
		"*.env*": "dotenv"
	},
	"files.autoSave": "afterDelay",
	"files.autoSaveDelay": 3000,
	"files.exclude": {
		"**/_site": true,
		"**/.DS_Store": true,
		"**/.git": true,
		"**/.hg": true,
		"**/.jekyll-cache": true,
		"**/.jekyll-metadata": true,
		"**/.map": true,
		"**/.sass-cache": true,
		"**/.svn": true,
		"**/dist": true,
		"**/node_modules": true,
		"**/out": true,
		"**/Thumbs.db": true
	},
	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
	"git.autofetch": true,
	"git.enableCommitSigning": true,
	"git.openRepositoryInParentFolders": "always",
	"java.format.enabled": true,
	"java.project.referencedLibraries": [
		"/usr/share/java/**/*.jar",
		"lib/**/*.jar"
	],
	"javascript.preferences.importModuleSpecifier": "relative",
	"javascript.suggest.paths": false,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"json.schemaDownload.trustedDomains": {
		"https://biomejs.dev": true,
		"https://json-schema.org/": true,
		"https://json.schemastore.org/": true,
		"https://raw.githubusercontent.com/": true,
		"https://schemastore.azurewebsites.net/": true,
		"https://www.schemastore.org/": true
	},
	"latex-utilities.message.update.show": false,
	"latex-workshop.formatting.latex": "tex-fmt",
	"latex-workshop.formatting.tex-fmt.args": [
		"--indent",
		"--indent-size",
		"--nowrap",
		"4"
	],
	"prettier.arrowParens": "always",
	"prettier.bracketSameLine": true,
	"prettier.bracketSpacing": true,
	"prettier.embeddedLanguageFormatting": "auto",
	"prettier.endOfLine": "lf",
	"prettier.experimentalTernaries": true,
	"prettier.htmlWhitespaceSensitivity": "css",
	"prettier.jsxSingleQuote": false,
	"prettier.printWidth": 80,
	"prettier.proseWrap": "preserve",
	"prettier.quoteProps": "consistent",
	"prettier.semi": true,
	"prettier.singleAttributePerLine": false,
	"prettier.singleQuote": false,
	"prettier.tabWidth": 4,
	"prettier.trailingComma": "all",
	"prettier.useTabs": true,
	"prettier.vueIndentScriptAndStyle": true,
	"redhat.telemetry.enabled": false,
	"scm.compactFolders": false,
	"search.exclude": {
		"**/_site": true,
		"**/.DS_Store": true,
		"**/.git": true,
		"**/.hg": true,
		"**/.jekyll-cache": true,
		"**/.jekyll-metadata": true,
		"**/.map": true,
		"**/.sass-cache": true,
		"**/.svn": true,
		"**/dist": true,
		"**/node_modules": true,
		"**/out": true,
		"**/Thumbs.db": true
	},
	"security.workspace.trust.enabled": false,
	"security.workspace.trust.startupPrompt": "never",
	"shellcheck.enable": true,
	"shellcheck.enableQuickFix": true,
	"shellcheck.ignoreFileSchemes": ["git", "gitfs", "output"],
	"shellcheck.run": "onType",
	"somesass.css.codeAction.enabled": true,
	"somesass.css.colors.enabled": true,
	"somesass.css.completion.enabled": true,
	"somesass.css.definition.enabled": true,
	"somesass.css.diagnostics.enabled": true,
	"somesass.css.documentSymbols.enabled": true,
	"somesass.css.foldingRanges.enabled": true,
	"somesass.css.highlights.enabled": true,
	"somesass.css.hover.enabled": true,
	"somesass.css.links.enabled": true,
	"somesass.css.references.enabled": true,
	"somesass.css.rename.enabled": true,
	"somesass.css.selectionRanges.enabled": true,
	"somesass.css.signatureHelp.enabled": true,
	"somesass.css.workspaceSymbol.enabled": true,
	"somesass.sass.completion.suggestFromUseOnly": true,
	"somesass.scss.codeAction.enabled": true,
	"somesass.scss.colors.enabled": true,
	"somesass.scss.colors.includeFromCurrentDocument": true,
	"somesass.scss.completion.css": true,
	"somesass.scss.completion.enabled": true,
	"somesass.scss.completion.includeFromCurrentDocument": true,
	"somesass.scss.completion.suggestFromUseOnly": true,
	"somesass.scss.definition.enabled": true,
	"somesass.scss.diagnostics.enabled": true,
	"somesass.scss.diagnostics.lint.enabled": true,
	"somesass.scss.documentSymbols.enabled": true,
	"somesass.scss.foldingRanges.enabled": true,
	"somesass.scss.highlights.enabled": true,
	"somesass.scss.hover.documentation": true,
	"somesass.scss.hover.enabled": true,
	"somesass.scss.links.enabled": true,
	"somesass.scss.references.enabled": true,
	"somesass.scss.rename.enabled": true,
	"somesass.scss.selectionRanges.enabled": true,
	"somesass.scss.signatureHelp.enabled": true,
	"somesass.scss.workspaceSymbol.enabled": true,
	"svelte.enable-ts-plugin": true,
	"svelte.plugin.css.enable": true,
	"svelte.plugin.html.enable": true,
	"svelte.plugin.typescript.enable": true,
	"telemetry.editStats.details.enabled": false,
	"telemetry.editStats.enabled": false,
	"telemetry.feedback.enabled": false,
	"telemetry.telemetryLevel": "off",
	"terminal.integrated.fontFamily": "Fira Code",
	"terminal.integrated.stickyScroll.enabled": false,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.preferences.importModuleSpecifier": "relative",
	"typescript.preferences.importModuleSpecifierEnding": "auto",
	"typescript.preferences.useAliasesForRenames": false,
	"typescript.suggest.paths": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"typescript.updateImportsOnPaste.enabled": true,
	"update.showReleaseNotes": false,
	"vscord.app.name": "VSCodium",
	"vscord.status.idle.check": false,
	"window.commandCenter": false,
	"window.customTitleBarVisibility": "never",
	"window.menuBarVisibility": "compact",
	"window.menuStyle": "native",
	"window.titleBarStyle": "native",
	"workbench.colorTheme": "GitHub Dark",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.layoutControl.enabled": false,
	"workbench.navigationControl.enabled": false,
	"workbench.productIconTheme": "material-product-icons",
	"workbench.secondarySideBar.defaultVisibility": "hidden",
	"workbench.startupEditor": "newUntitledFile",
	"workbench.tips.enabled": false,
	"workbench.tree.indent": 16,
	"yaml.schemas": {
		"https://raw.githubusercontent.com/compose-spec/compose-spec/master/schema/compose-spec.json": "*compose*.yml"
	}
}
